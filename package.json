{
  "name": "start-react-redux-app",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js",
    "start:fallback": "cross-env BROWSERS_ENV=legacy webpack-dev-server --config webpack.config.js",
    "build": "cross-env NODE_ENV=production BROWSERS_ENV=only_modern webpack --config webpack.config.js",
    "build:fallback": "npm run build:legacy && npm run build:modern",
    "build:modern": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:legacy": "cross-env NODE_ENV=production BROWSERS_ENV=legacy webpack --config webpack.config.js",
    "lint:js": "eslint --max-warnings=0 --no-ignore src/",
    "lint:styles": "stylelint \"src/**/*.less\" --config .stylelintrc.js",
    "format:js": "eslint --fix \"src/**/*.js\" ",
    "format:styles": "stylelint \"src/**/*.less\" --fix --config .stylelintrc.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --colors",
    "generate": "plop",
    "storybook": "start-storybook",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build-report": "webpack-bundle-analyzer --port 8888 dist/stats.json",
    "postinstall": "npm run cache:reset",
    "cache:reset": "npx rimraf ./node_modules/.cache/"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.less": [
      "stylelint --config .stylelintrc.js",
      "stylelint --fix --config .stylelintrc.js",
      "git add"
    ],
    "*.(js|jsx)": [
      "eslint --max-warnings=0 --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-export-default-from": "7.7.4",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "@sambego/storybook-state": "1.3.6",
    "@sambego/storybook-styles": "1.0.0",
    "@storybook/addon-actions": "5.3.2",
    "@storybook/addon-knobs": "5.3.2",
    "@storybook/addon-notes": "5.3.2",
    "@storybook/addons": "5.3.2",
    "@storybook/react": "5.3.2",
    "@svgr/webpack": "5.0.1",
    "autodll-webpack-plugin": "0.4.2",
    "autoprefixer": "9.7.3",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "cache-loader": "4.1.0",
    "classnames": "2.2.6",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "core-js": "3.6.1",
    "cross-env": "6.0.3",
    "css-loader": "3.4.0",
    "dotenv": "8.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "error-overlay-webpack-plugin": "0.4.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-compat": "3.7.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.4.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-optimize-regex": "1.2.0",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "eslint-plugin-sonarjs": "0.5.0",
    "eslint-plugin-sort-destructure-keys": "1.3.4",
    "external-svg-sprite-loader": "5.0.1",
    "fast-async": "6.3.8",
    "file-loader": "5.0.2",
    "friendly-errors-webpack-plugin": "1.7.0",
    "gh-pages": "2.1.1",
