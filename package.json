  "engines": {
    "node": ">=10.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.less": [
      "stylelint --config .stylelintrc.js",
      "stylelint --fix --config .stylelintrc.js",
      "git add"
    ],
    "*.(js|jsx)": [
      "eslint --max-warnings=0 --fix --no-ignore src/",
      "git add"
    ]
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development BROWSERS=modern BACKEND_URL=https://some.url.ru webpack-dev-server",
    "build:modern": "cross-env NODE_ENV=production BROWSERS=modern BACKEND_URL=https://some.url.ru webpack --progress",
    "build:old": "cross-env NODE_ENV=production BROWSERS=old BACKEND_URL=https://some.url.ru webpack --progress",
    "prod": "rimraf ./dist && npm run build:modern & npm run build:old",
    "clean": "rimraf ./dist",
    "lint:scripts": "eslint --max-warnings=0 --no-ignore src/",
    "format:scripts": "eslint --fix \"src/**/*.js\" ",
    "lint:styles": "stylelint \"src/**/*.less\" --config .stylelintrc.js",
    "format:styles": "stylelint --fix \"src/**/*.less\" --config .stylelintrc.js",
    "test": "cross-env BROWSERS=modern jest",
    "test:watch": "npm test -- --watch",
    "generate": "plop",
    "new:component": "plop component",
    "new:stories": "plop story",
    "new:page": "plop page",
    "new:hoc": "plop hoc",
    "new:store:section": "plop store",
    "storybook": "cross-env BROWSERS=modern start-storybook",
    "storybook:build": "npm run clean:storybook && cross-env BROWSERS=modern build-storybook -c .storybook -o .out",
    "clean:storybook": "rimraf ./.out",
    "predeploy": "rimraf ./dist && npm run build:modern",
    "deploy": "gh-pages -d dist",
    "build:report": "webpack-bundle-analyzer --port 47777 dist/stats.json"
  }
}
